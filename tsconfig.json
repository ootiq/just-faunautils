{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"outDir": "./lib"
	},
	"include": ["./src"],
	"exclude": ["lib", "node_modules"]
}
